{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "plugin": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "image": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "image"
        ]
      },
      "check": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "displayname": {
            "type": "string"
          },
          "pass": {
            "type": "boolean"
          }
        },
        "required": [
          "name",
          "displayname"
        ]
      },
      "step": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "pass": {
            "type": "boolean"
          },
          "level": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "pass",
          "level",
          "message"
        ]
      }
    },
    "required": [
      "plugin",
      "check"
    ]
  }
}
